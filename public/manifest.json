{
  "name": "Dhikr Chrome Extension",
  "description": "Daily Dhikr",
  "version": "1.0",
  "manifest_version": 3,
  "chrome_url_overrides":{
    "newtab":"index.html"
  },
  "permissions": [
    "topSites",
    "chrome://favicon"
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "content_security_policy": {
    "script-src": "self",
    "object-src": "self",
    "img-src": "self"
  }
}
