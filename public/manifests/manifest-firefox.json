{
  "name": "Dhikr Browser Extension",
  "description": "Daily Dhikr",
  "version": "3.2",
  "manifest_version": 3,
  "chrome_url_overrides":{
    "newtab":"index.html"
  },
  "action":{
    "default_icon": {
      "16":"assets/dhikr_16x16.png",
      "64":"assets/dhikr_64x64.png",
      "128":"assets/dhikr_128x128.png"
    }
  },
  "permissions": [
    "topSites",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self';object-src 'self';"
  }
}

